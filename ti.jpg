        <style type="text/css">
            .modal_loader {
                display:    none;
                position:   fixed;
                z-index:    1100;
                top:        0;
                left:       0;
                height:     100%;
                width:      100%;
                background: rgba( 255, 255, 255, .8 )
                    url('https://pmb.polindra.ac.id/resource/doc/images/icon/loader.gif')
                    50% 50%
                    no-repeat;
            }
            body.loading {
                overflow: hidden;
            }
            body.loading .modal_loader {
                display: block;
            }
        </style>
        <script type="text/javascript">
            $(document).ready(function() {
                $body = $("body");
                $(document).on({
                    ajaxStart: function() {
                        $body.addClass("loading");
                    },
                    ajaxStop: function() {
                        $body.removeClass("loading");
                    }
                });
                // cek gelombang
                $('#gelombang').click(function() {
                    // ajax process
                    $.ajax({
                        url: 'https://pmb.polindra.ac.id/index.php/login/welcome/cek_gelombang',
                        dataType: 'json',
                        success: function(result) {
                            if (result.status === 'success') {
                                // berhasil, redirect sesuai dengan datanya
                                $("#signin-box").modal('toggle');
                                $("#reg-gelombang").show();
                                $("#alert-gelombang").html(result.message)
                            } else if (result.status === 'notif') {
                                $("#signin-box").modal('toggle');
                                $("#reg-notif").show();
                                $("#alert-notif").html(result.message)
                            }
                            $('#daftar-click').prop("disabled", false);
                        },
                    });
                });
                // trigger form dialog
                $('#sign-click').click(function() {
                    $('#reg-box').modal('toggle');
                });
                $('#reg-click').click(function() {
                    $('#signin-box').modal('toggle');
                });
                // ajax login
                $('#form-login').submit(function() {
                    // set variable
                    var login_kode = $("#login_kode").val();
                    var login_password = $("#login_password").val();
                    // var captcha_2 = $("#captcha_2").val();
                    // set form data
                    var formData = new FormData();
                    formData.append('login_kode', login_kode);
                    formData.append('login_password', login_password);
                    // formData.append('captcha_2', captcha_2);
                    // ajax process
                    $.ajax({
                        url: 'https://pmb.polindra.ac.id/index.php/login/welcome/ajax_login',
                        dataType: 'json',
                        type: 'POST',
                        beforeSend: function() {
                            $('#login-click').prop("disabled", true);
                        },
                        success: function(result) {
                            if (result.status === 'success') {
                                // berhasil, redirect sesuai dengan datanya
                                window.location.href = 'https://pmb.polindra.ac.id/index.php/login/welcome/login_pendaftaran/' + result.message + '';
                            } else {
                                $("#login-error").show();
                                $("#login_kode").val('');
                                $("#login_password").val('');
                                // $("#captcha_2").val('');
                                $("#alert-login").html(result.message);
                            }
                            $('#login-click').prop("disabled", false);
                        },
                        error: function() {
                            $("#login-error").show();
                            $('#login-click').prop("disabled", false);
                            $("#login_kode").val('');
                            $("#login_password").val('');
                            // $("#captcha_2").val('');
                            $("#alert-login").html(result.message);
                        },
                        complete: function() {
                            $('#login-click').prop("disabled", false);
                            $("#login_kode").val('');
                            $("#login_password").val('');
                            // $("#captcha_2").val('');
                        },
                        data: formData,
                        cache: false,
                        contentType: false,
                        processData: false
                    }, 'json');
                    // return false
                    return false;
                });
                // ajax registration
                $('#form-registration').submit(function() {
                    // set variable
                    var nama = $("#reg-nama").val();
                    var nisn = $("#reg-nisn").val();
                    var email = $("#reg-email").val();
                    // var captcha = $("#captcha").val();
                    // set form data
                    var formData = new FormData();
                    formData.append('nama', nama);
                    formData.append('nisn', nisn);
                    formData.append('email', email);
                    // formData.append('captcha', captcha);
                    // ajax process
                    $.ajax({
                        url: 'https://pmb.polindra.ac.id/index.php/login/welcome/ajax_pendaftaran',
                        dataType: 'json',
                        type: 'POST',
                        beforeSend: function() {
                            $('#daftar-click').prop("disabled", true);
                        },
                        success: function(result) {
                            if (result.status === 'success') {
                                // berhasil, redirect sesuai dengan datanya
                                $("#reg-gelombang").show();
                                $("#reg-nama").val('');
                                $("#reg-nisn").val('');
                                $("#reg-email").val('');
                                // $("#captcha").val('');
                                $("#reg-notif").hide();
                                $("#reg-error").hide();
                                $("#alert-gelombang").html(result.message);
                            } else if (result.status === 'error') {
                                $("#reg-error").show();
                                $("#reg-nama").val('');
                                $("#reg-nisn").val('');
                                $("#reg-email").val('');
                                // $("#captcha").val('');
                                $("#alert-error").html(result.message);
                                $("#reg-gelombang").hide();
                                $("#reg-notif").hide();
                            } else if (result.status === 'notif') {
                                $("#reg-notif").show();
                                $("#reg-nama").val('');
                                $("#reg-nisn").val('');
                                $("#email").val('');
                                // $("#captcha").val('');
                                $("#reg-notif").html(result.message);
                                $("#reg-gelombang").hide();
                            }
                            $('#daftar-click').prop("disabled", false);
                        },
                        error: function() {
                            $("#reg-error").show();
                            $('#daftar-click').prop("disabled", false);
                            $("#reg-nama").val('');
                            $("#reg-nisn").val('');
                            $("#reg-email").val('');
                            // $("#captcha").val('');
                            $("#alert-error").html('Mohon maaf, proses pendaftaran gagal!');
                            $("#reg-gelombang").hide();
                            $("#reg-notif").hide();
                        },
                        complete: function() {
                            $('#daftar-click').prop("disabled", false);
                            $("#reg-nama").val('');
                            $("#reg-nisn").val('');
                            $("#reg-email").val('');
                            // $("#captcha").val('');
                        },
                        data: formData,
                        cache: false,
                        contentType: false,
                        processData: false
                    }, 'json');
                    // return false
                    return false;
                });
            });
        </script>
